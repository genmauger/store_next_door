<% if user_signed_in? %>
  <%= current_user.email %>
  <%= image_tag current_user.profile.image_url(:medium), :class => "nav_style_image" unless current_user.profile.image.nil? %>
  <%= link_to 'Logout', destroy_user_session_path, method: :delete %>
  <% else %>
    <%= link_to 'Login', new_user_session_path %>
<% end  %>